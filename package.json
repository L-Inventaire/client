    "react-string-replace": "^1.1.1",
    "react-superellipse": "^0.0.6",
    "recoil": "^0.7.2",
    "seedrandom": "^3.0.5",
    "socket.io-client": "^4.5.0",
    "tailwind-merge": "^2.3.0",
    "tippy.js": "^6.3.7",
    "totp-generator": "^1.0.0",
    "usehooks-ts": "^3.1.0",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.27.7",
    "@types/luxon": "^3.4.2",
