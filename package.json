{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-frontend": "export PORT=3006 && craco start",
    "start-tailwind": "npx tailwindcss -i ./src/tailwind.css -o ./public/styles/tailwind.css --watch",
    "start": "concurrently \"npm run start-frontend\" \"npm run start-tailwind\"",
    "dev": "concurrently \"npm run start-frontend\" \"npm run start-tailwind\"",
    "update-tailwind-import": "sed -i '' -e \"s/tailwind.css?t=[^\\\"]*\\\"/tailwind.css?t=$(date +%s)\\\"/g\" ./public/index.html",
    "build": "export CI=true && craco build",
    "linter": "export CI=true && eslint src/ --max-warnings 0",
    "test": "craco test",
    "eject": "craco eject"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^6.4.3",
    "@documenso/embed-react": "^0.1.0",
    "@editorjs/checklist": "^1.6.0",
    "@editorjs/delimiter": "^1.4.0",
    "@editorjs/editorjs": "^2.29.0",
    "@editorjs/inline-code": "^1.5.0",
    "@editorjs/link": "^2.6.2",
    "@editorjs/paragraph": "^2.11.3",
    "@editorjs/quote": "^2.6.0",
    "@fluentui/react": "^8.118.0",
    "@fluentui/react-file-type-icons": "^8.11.9",
    "@fluentui/react-icons": "^2.0.239",
    "@headlessui/react": "^1.6.0",
    "@heroicons/react": "^2.1.3",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/themes": "^3.1.3",
    "@sentry/react": "^7.35.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-query-devtools": "^5.40.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/color": "^3.0.6",
    "@types/jspdf": "^2.0.0",
    "@types/lodash": "^4.14.182",
    "@types/quill": "^2.0.14",
    "@types/react": "^18.0.8",
    "@types/react-custom-scrollbars": "^4.0.13",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.3",
    "@types/react-phone-number-input": "^3.0.17",
    "@types/seedrandom": "^3.0.2",
    "@types/totp-generator": "^0.0.8",
    "@uidotdev/usehooks": "^2.4.1",
    "classnames": "^2.3.2",
    "color": "^4.2.3",
    "editorjs-inline-spoiler-tool": "^1.0.1",
    "eslint": "^8.48.0",
    "fuse.js": "^7.0.0",
    "html2pdf.js": "0.9.3",
    "i18next": "^21.8.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "npm": "^9.1.1",
    "postcss-prefix-selector": "^1.16.1",
    "qrcode.react": "^3.1.0",
    "quill": "^2.0.2",
    "react": "^18.1.0",
    "react-circular-progressbar": "^2.1.0",
    "react-custom-scrollbars": "^4.2.1",
