{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-frontend": "export PORT=3006 && craco start",
    "start-tailwind": "npx tailwindcss -i ./src/tailwind.css -o ./public/styles/tailwind.css --watch",
    "start": "concurrently \"npm run start-frontend\" \"npm run start-tailwind\"",
    "update-tailwind-import": "sed -i '' -e \"s/tailwind.css?t=[^\\\"]*\\\"/tailwind.css?t=$(date +%s)\\\"/g\" ./public/index.html",
    "build": "export CI=true && craco build",
    "linter": "export CI=true && eslint src/",
    "test": "craco test",
    "eject": "craco eject"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^6.4.3",
    "@editorjs/checklist": "^1.6.0",
    "@editorjs/code": "^2.9.0",
    "@editorjs/delimiter": "^1.4.0",
    "@editorjs/editorjs": "^2.29.0",
    "@editorjs/embed": "^2.7.0",
    "@editorjs/inline-code": "^1.5.0",
    "@editorjs/link": "^2.6.2",
    "@editorjs/list": "^1.9.0",
    "@editorjs/marker": "^1.4.0",
    "@editorjs/paragraph": "^2.11.3",
    "@editorjs/quote": "^2.6.0",
    "@editorjs/table": "^2.3.0",
    "@editorjs/underline": "^1.1.0",
    "@headlessui/react": "^1.6.0",
    "@heroicons/react": "^1.0.6",
    "@sentry/react": "^7.35.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tanstack/react-query": "^5.28.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.8",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.3",
    "@types/react-phone-number-input": "^3.0.17",
    "@types/seedrandom": "^3.0.2",
    "@types/totp-generator": "^0.0.8",
    "@uidotdev/usehooks": "^2.4.1",
    "classnames": "^2.3.2",
    "editorjs-inline-spoiler-tool": "^1.0.1",
    "eslint": "^8.48.0",
    "fuse.js": "^7.0.0",
    "i18next": "^21.8.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "npm": "^9.1.1",
    "qrcode.react": "^3.1.0",
    "react": "^18.1.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.1.0",
    "react-editor-js": "=2.0.5",
    "react-fastclick": "^3.0.2",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^11.18.1",
    "react-phone-number-input": "^3.3.7",
    "react-router-dom": "6",
    "react-scripts": "5.0.1",
    "react-string-replace": "^1.1.1",
    "react-superellipse": "^0.0.6",
    "recoil": "^0.7.2",
    "seedrandom": "^3.0.5",
    "simplebar-react": "^3.2.4",
    "socket.io-client": "^4.5.0",
    "tailwind-merge": "^2.2.2",
    "tippy.js": "^6.3.7",
    "totp-generator": "^1.0.0",
    "usehooks-ts": "^3.1.0",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.27.7",
    "concurrently": "^8.2.0",
    "sass": "^1.66.1",
    "tailwindcss": "^3.0.24",
    "typescript": "=5.1.6"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  }
}
