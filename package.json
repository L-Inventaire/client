    "tailwindcss": "^3.0.24",
    "typescript": "=5.1.6"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "cssnano": {
    "preset": [
      "default",
      {
        "calc": false
      }
    ]
  }
